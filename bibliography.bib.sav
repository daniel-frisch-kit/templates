@Article{Chen2017_ApproximatingPlanarConformalMapsUsingRegularPolygonalMeshes,
  author   = {Chen, Renjie and Gotsman, Craig},
  journal  = {Computer Graphics Forum},
  title    = {Approximating {Planar} {Conformal} {Maps} {Using} {Regular} {Polygonal} {Meshes}},
  year     = {2017},
  issn     = {1467-8659},
  note     = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/cgf.13157},
  number   = {8},
  pages    = {629--642},
  volume   = {36},
  abstract = {Continuous conformal maps are typically approximated numerically using a triangle mesh which discretizes the plane. Computing a conformal map subject to user-provided constraints then reduces to a sparse linear system, minimizing a quadratic ‘conformal energy’. We address the more general case of non-triangular elements, and provide a complete analysis of the case where the plane is discretized using a mesh of regular polygons, e.g. equilateral triangles, squares and hexagons, whose interiors are mapped using barycentric coordinate functions. We demonstrate experimentally that faster convergence to continuous conformal maps may be obtained this way. We provide a formulation of the problem and its solution using complex number algebra, significantly simplifying the notation. We examine a number of common barycentric coordinate functions and demonstrate that superior approximation to harmonic coordinates of a polygon are achieved by the Moving Least Squares coordinates. We also provide a simple iterative algorithm to invert barycentric maps of regular polygon meshes, allowing to apply them in practical applications, e.g. for texture mapping.},
  doi      = {10.1111/cgf.13157},
  file     = {Snapshot:/media/frisch/DATEN_SSD/ISAS/versch/zotero/data/storage/PWVVJLUB/cgf.html:text/html;Full Text PDF:/media/frisch/DATEN_SSD/ISAS/versch/zotero/data/storage/HLJ8R522/Chen und Gotsman - 2017 - Approximating Planar Conformal Maps Using Regular .pdf:application/pdf},
  keywords = {conformal, conformal maps, I.3.8 Computer Graphics: Applications, regular polygonal mesh},
  language = {en},
  url      = {https://onlinelibrary.wiley.com/doi/abs/10.1111/cgf.13157},
  urldate  = {2021-12-09},
}


@Misc{DanielFrisch2021_FUSION2021DeterministicGaussianSamplingWithGeneralizedFibonacciGrids,
  author   = {{Daniel Frisch}},
  month    = jun,
  title    = {{FUSION} 2021 – {Deterministic} {Gaussian} {Sampling} with {Generalized} {Fibonacci} {Grids}},
  year     = {2021},
  abstract = {Title: Deterministic Gaussian Sampling with Generalized Fibonacci Grids

Authors: Daniel Frisch, Uwe D. Hanebeck 

Abstract: We propose a simple and efficient method to obtain unweighted deterministic samples of the multivariate Gaussian density. It allows to place a large number of homogeneously placed samples even in high-dimensional spaces. There is a demand for large high-quality sample sets in many nonlinear filters. The Smart Sampling Kalman Filter (S2KF), for example, uses many samples and is an extension of the Unscented Kalman Filter (UKF) that is limited due to its small sample set. Generalized Fibonacci grids have the property that if stretched or compressed along certain directions, the grid points keep approximately equal distances to all their neighbors. This can be exploited to easily obtain deterministic samples of arbitrary Gaussians. As the computational effort to generate these anisotropically scalable point sets is low, generalized Fibonacci grid sampling appears to be a great new source of large sample sets in high-quality state estimation.},
  url      = {https://www.youtube.com/watch?v=y_2PhQANmpc},
  urldate  = {2021-12-22},
}


@Article{Kacwin2021_NumericalPerformanceOfOptimizedFrolovLatticesInTensorProductReproducingKernelSobolevSpaces,
  author   = {Kacwin, Christopher and Oettershagen, Jens and Ullrich, Mario and Ullrich, Tino},
  journal  = {Foundations of Computational Mathematics},
  title    = {Numerical {Performance} of {Optimized} {Frolov} {Lattices} in {Tensor} {Product} {Reproducing} {Kernel} {Sobolev} {Spaces}},
  year     = {2021},
  issn     = {1615-3383},
  month    = jun,
  number   = {3},
  pages    = {849--889},
  volume   = {21},
  abstract = {In this paper, we deal with several aspects of the universal Frolov cubature method, which is known to achieve optimal asymptotic convergence rates in a broad range of function spaces. Even though every admissible lattice has this favorable asymptotic behavior, there are significant differences concerning the precise numerical behavior of the worst-case error. To this end, we propose new generating polynomials that promise a significant reduction in the integration error compared to the classical polynomials. Moreover, we develop a new algorithm to enumerate the Frolov points from non-orthogonal lattices for numerical cubature in the d-dimensional unit cube \$\$[0,1]{\textasciicircum}d\$\$. Finally, we study Sobolev spaces with anisotropic mixed smoothness and compact support in \$\$[0,1]{\textasciicircum}d\$\$and derive explicit formulas for their reproducing kernels. This allows for the simulation of exact worst-case errors which numerically validate our theoretical results.},
  doi      = {10.1007/s10208-020-09463-y},
  file     = {Springer Full Text PDF:C\:\\Users\\Mama\\Zotero\\storage\\DARB5UMT\\Kacwin et al. - 2021 - Numerical Performance of Optimized Frolov Lattices.pdf:application/pdf},
  language = {en},
  url      = {https://doi.org/10.1007/s10208-020-09463-y},
  urldate  = {2022-01-08},
}



